   "source": [
    "# LCM of two numbers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "a7a570b2",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "a:15\n",
      "b:30\n",
      "lcm is: 30.0\n"
     ]
    }
   ],
   "source": [
    "a=int(input('a:'))\n",
    "b=int(input('b:'))\n",
    "gcd=0\n",
    "for i in range(1,min(a,b)+1):\n",
    "    if a%i==0 and b%i==0:\n",
    "        gcd=i\n",
    "lcm = a*b/gcd  \n",
    "print('lcm is:',lcm)"
   ]
