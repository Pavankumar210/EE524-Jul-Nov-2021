    "import numpy as np\n",
    "a=np.array([73, 51, 1, 39, 76, 81, 19, 36, 28, 29, 59, 79, 25, 75])\n",
    "for k in range(0, 13):\n",
    "    for j in range(0, 13):\n",
    "        if a[j]>a[j+1]:\n",
    "            a[j],a[j+1]=a[j+1],a[j]\n",
    "print(a)"
