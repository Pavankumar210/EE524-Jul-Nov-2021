     "name": "stdout",
     "output_type": "stream",
     "text": [
      "descending order\n",
      " [100  81  79  76  75  59  51  39  36  29  28  25  19  12   2]\n"
     ]
    }
   ],
   "source": [
    "l=np.array([100,51,12,39,76,81,19,36,28,29,59,79,25,75,2])\n",
    "for k in range(0,14):\n",
    "    for j in range(0,14):\n",
    "        if l[j]<l[j+1]:\n",
    "            l[j],l[j+1]=l[j+1],l[j]\n",
    "print('descending order\\n',l)"
   ]
